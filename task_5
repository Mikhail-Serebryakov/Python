class Stationery:
    def __init__(self, title):
        self.title = title

    def draw(self):
        return print('Запуск отрисовки!')

class Pen(Stationery):
    def __init__(self, title):
        super().__init__(title)


    def draw(self):
        return print('Сейчас пишим ручкой...')

class Pencil(Stationery):
    def __init__(self, title):
        super().__init__(title)


    def draw(self):
        return print('Сейчас рисуем карандашом...')

class Handle(Stationery):
    def __init__(self, title):
        super().__init__(title)


    def draw(self):
        return print('Сейчас выделяем текст маркером...')


pen = Pen('Ручка')
pen.draw()
pencil = Pencil('Карандаш')
pencil.draw()
handle = Handle('Маркер')
handle.draw()
