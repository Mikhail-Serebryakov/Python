class Worker:
    name = 'Mikhail'
    surname = 'Serebryakov'
    position = 'Data science'
    _income = {"wage": 170000, "bonus": 120000}

class Position(Worker):
    def get_full_name(self):
        return print(self.name, self.surname)

    def get_total_income(self):
        return print(self._income.get('wage') + self._income.get('bonus'), 'Тысяч рублей')

x = Position()
x.get_full_name()
x.get_total_income()

# Так же, как я понимаю можно было зделать через __init__ вначале, и атрибуты вносить в методе