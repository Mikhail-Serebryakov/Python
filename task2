name = input('Введите имя: ')
surname = input('Введите фамилию: ')
year_of_birth = input('Введите год рождения: ')
city_of_residence = input('Введите город проживания: ')
email = input('Введите email: ')
phone = input('Введите телефон: ')

def information(name, surname, year_of_birth, city_of_residence, email, phone):
    print(f'Имя {name}; Фамилия {surname}; Год рождения {year_of_birth}; Город {city_of_residence}; Электронный адрес {email}; Телефон {phone}')


information(name, surname, year_of_birth, city_of_residence, email, phone)